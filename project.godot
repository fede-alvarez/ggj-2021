; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="GGJ_2021"
run/main_scene="res://scenes/Menu.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://scripts/Globals.gd"
SoundsManager="*res://scripts/SoundsManager.gd"
MusicManager="*res://scripts/MusicManager.gd"

[display]

window/size/width=320
window/size/height=180
window/size/resizable=false
window/size/test_width=960
window/size/test_height=540
mouse_cursor/custom_image="res://assets/main_cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 11, 2 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[locale]

translations=PoolStringArray( "res://assets/localization/AFD_Translations - base.es.translation", "res://assets/localization/AFD_Translations - base.en.translation" )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
